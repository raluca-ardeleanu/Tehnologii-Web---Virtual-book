   .then(res => {\n                      this.setState({message : 'Note added successfully.'});\n                      this.props.history.push('/home');\n                  });\n          }\n\n    /**\n     * @property Jodit jodit instance of native Jodit\n     */\n\tjodit;\n\tsetRef = jodit => this.jodit = jodit;\n\n\tconfig = {\n\t\treadonly: false,\ntoolbarButtonSize: 'large',\n                  uploader: {\n                    url: '/manage/upload',\n                    insertImageAsBase64URI: false,\n                    imagesExtensions: ['jpg', 'png', 'jpeg', 'gif'],\n                    //headers: {\"token\":`${db.token}`},\n                    filesVariableName: function (t) {\n                      return 'files[' + t + ']';\n                    }, //\"files\",\n                    withCredentials: false,\n                    pathVariableName: 'path',\n                    format: 'json',\n                    method: 'POST',\n                    prepareData: function (formdata) {\n                      return formdata;\n                    },\n                    isSuccess: function (e) {\n                      debugger;\n                      return e.success;\n                    },\n                    getMessage: function (e) {\n                      return void 0 !== e.data.messages && Array.isArray(e.data.messages)\n                        ? e.data.messages.join('')\n                        : '';\n                    },\n                    process: function (resp: any) { //success callback transfrom data to defaultHandlerSuccess use.it's up to you.\n                      let files = [];\n                      files.unshift(resp.data);\n                      return {\n                        files: resp.data,\n                        error: resp.msg,\n                        msg: resp.msg,\n                      };\n                    }\n                  },\n\n\t}\n    render() {\n        return (\n        <div>\n         <Select\n           labelId=\"demo-simple-select-helper-label\"\n           id=\"demo-simple-select-helper\"\n           value={this.state.idMaterie}\n          onChange={this.handleChange}\n         >\n           <MenuItem value=\"\">\n             <em>None</em>\n           </MenuItem>\n           {this.state.materii.map(row => (<MenuItem value={row.idMaterie}>{row.numeMaterie}</MenuItem>))}\n         </Select>\n        <TextField type=\"text\" label=\"Titlu\" fullWidth margin=\"normal\" name=\"titlu\" value={this.state.titlu} onChange={this.onChange}/>\n         <TextField type=\"text\" label=\"Label\" fullWidth margin=\"normal\" name=\"label\" value={this.state.label} onChange={this.onChange}/>\n        <Button variant=\"contained\" color=\"primary\" onClick={this.save}>Save</Button>\n            <JoditEditor\n            \teditorRef={this.setRef}\n                value={this.state.content}\n                config={this.config}\n                onChange={this.updateContent}\n            />\n\n          </div>\n        );\n    }\n}\n\nexport default TextEditor;","/Users/andreiardeleanu/Downloads/Tehnologii-Web---Virtual-book-main/proiect/front-end/virtualbook/src/components/List/ListNotes.js",[],"/Users/andreiardeleanu/Downloads/Tehnologii-Web---Virtual-book-main/proiect/front-end/virtualbook/src/components/Agenda/Agenda.js",["72","73","74","75"],{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","severity":1,"message":"81","line":10,"column":17,"nodeType":"82","endLine":10,"endColumn":34},{"ruleId":"83","severity":1,"message":"84","line":5,"column":8,"nodeType":"85","messageId":"86","endLine":5,"endColumn":14},{"ruleId":"83","severity":1,"message":"87","line":3,"column":10,"nodeType":"85","messageId":"86","endLine":3,"endColumn":16},{"ruleId":"83","severity":1,"message":"88","line":2,"column":8,"nodeType":"85","messageId":"86","endLine":2,"endColumn":11},{"ruleId":"83","severity":1,"message":"89","line":1,"column":16,"nodeType":"85","messageId":"86","endLine":